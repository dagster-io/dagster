# Example environment variables for Buildkite integration
# Copy this file to .env and fill in your actual values

# Buildkite Analytics Token - get this from your Buildkite Analytics settings
BUILDKITE_ANALYTICS_TOKEN=your_api_token_here

# Buildkite environment (set to "true" when running in CI)
BUILDKITE=false

# These are typically set automatically by Buildkite CI
BUILDKITE_BUILD_ID=
BUILDKITE_JOB_ID=
BUILDKITE_COMMIT=
BUILDKITE_ORGANIZATION_SLUG=dagster
BUILDKITE_PIPELINE_SLUG=unit-tests
BUILDKITE_BRANCH=
BUILDKITE_BUILD_NUMBER=
