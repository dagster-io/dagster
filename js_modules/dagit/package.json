{
  "name": "dagit",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@blueprintjs/core": "^3.9.0",
    "@blueprintjs/icons": "^3.3.0",
    "@blueprintjs/select": "^3.3.0",
    "@vx/gradient": "^0.0.165",
    "@vx/group": "^0.0.170",
    "@vx/legend": "^0.0.170",
    "@vx/network": "^0.0.170",
    "@vx/responsive": "^0.0.165",
    "@vx/scale": "^0.0.165",
    "@vx/shape": "^0.0.170",
    "ajv": "^6.5.5",
    "amator": "^1.1.0",
    "apollo-boost": "^0.1.27",
    "apollo-link-ws": "^1.0.12",
    "codemirror": "^5.40.2",
    "d3-hierarchy": "^1.1.6",
    "dagre": "^0.8.2",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "highlight.js": "^9.13.1",
    "immer": "^1.8.0",
    "lodash": "^4.17.11",
    "query-string": "^6.1.0",
    "react": "^16.4.2",
    "react-apollo": "^2.4.1",
    "react-codemirror2": "^5.1.0",
    "react-dom": "^16.4.2",
    "react-markdown": "^3.4.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-virtualized": "^9.21.0",
    "styled-components": "^3.3.3",
    "subscriptions-transport-ws": "^0.9.15",
    "yaml": "^1.0.1",
    "yaml-language-server": "^0.0.19"
  },
  "devDependencies": {
    "@types/ajv": "^1.0.0",
    "@types/codemirror": "^0.0.60",
    "@types/dagre": "^0.7.40",
    "@types/graphql": "^14.0.5",
    "@types/highlight.js": "^9.12.3",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.120",
    "@types/node": "^10.5.5",
    "@types/prop-types": "^15.5.6",
    "@types/query-string": "^6.1.0",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "@types/react-router": "^4.0.30",
    "@types/react-router-dom": "^4.3.0",
    "@types/react-test-renderer": "^16.0.2",
    "@types/react-virtualized": "^9.18.11",
    "@types/ws": "^6.0.1",
    "@types/yaml": "^1.0.0",
    "apollo": "^2.3.1",
    "babel-core": "^6.23.0",
    "babel-jest": "^23.4.2",
    "babel-plugin-graphql-tag": "^1.6.0",
    "jest": "^23.5.0",
    "prettier": "^1.14.0",
    "react-scripts-ts": "2.16.0",
    "react-test-renderer": "^16.4.2",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.1",
    "typescript-babel-jest": "^1.0.5"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "build-for-python": "npm run build && cd ../../python_modules/dagit/dagit && mkdir -p webapp && cp -r ../../../js_modules/dagit/build ./webapp/",
    "eject": "react-scripts-ts eject",
    "prettier": "prettier --write \"./src/**/*.tsx\"",
    "test": "npm run ts && npm run jest && npm run check-prettier",
    "jest": "jest",
    "check-prettier": "prettier --list-different \"./src/**/*.tsx\"",
    "ts": "tsc -p .",
    "download-schema": "apollo schema:download --endpoint $REACT_APP_GRAPHQL_URI ./src/schema.json",
    "generate-types": "apollo codegen:generate --queries \"./src/**/*.tsx\" --target typescript types --schema ./src/schema.json",
    "watch-types": "apollo codegen:generate --queries \"./src/**/*.tsx\" --target typescript types --schema ./src/schema.json --watch"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|css)$": "<rootDir>/jest/fileTransformer.js"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|css)$": "<rootDir>/jest/fileTransformer.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "testRegex": "/__tests__/.*\\.test\\.(ts|tsx)$",
    "testURL": "http://localhost"
  }
}
