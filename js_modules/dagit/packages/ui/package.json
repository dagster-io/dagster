{
  "name": "@dagster-io/ui",
  "version": "1.0.4",
  "description": "Dagster UI Component Library",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "sideEffects": false,
  "scripts": {
    "prepack": "yarn build",
    "prepublish": "yarn lint && yarn ts && yarn jest",
    "build": "rm -rf lib && tsc -p ./tconfig.build.json && yarn rollup -c rollup.config.js",
    "generate-graphql": "cp -r ../core/src/scripts/generateGraphQLTypes.ts ./src/scripts/generateGraphQLTypes.ts && ts-node -O '{\"module\": \"commonjs\"}' ./src/scripts/generateGraphQLTypes.ts",
    "lint": "eslint src/ --ext=.tsx,.ts,.js --fix -c .eslintrc.js",
    "jest": "jest",
    "ts": "tsc -p .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "@blueprintjs/core": "^3.45.0",
    "@blueprintjs/popover2": "0.10.1",
    "@blueprintjs/select": "^3.16.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "styled-components": "^5.3.3"
  },
