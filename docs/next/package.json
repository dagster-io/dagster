  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NEXT_PUBLIC_ENV=development PORT=3001 next-remote-watch ../content",
    "build": "next build",
    "build-master": "VERSIONING_DISABLED=true next build",
