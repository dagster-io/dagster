# Google Gemini Configuration
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-2.0-flash-exp
GEMINI_JUDGE_MODEL=gemini-2.0-flash-exp
GEMINI_MAX_TOKENS=8192
GEMINI_TEMPERATURE=0.7

# Connections Data Configuration
CONNECTIONS_DATA_PATH=./data/Connections_Data.csv

# Dagster Configuration
DAGSTER_HOME=/tmp/dagster_home
DAGSTER_HOST=localhost
DAGSTER_PORT=3000

# Connections Puzzle Configuration
TRAIN_TEST_SPLIT=0.25
EVAL_SUBSET_SIZE=10
MAX_PUZZLE_ATTEMPTS=6
MAX_INVALID_RESPONSES=3
NUM_THREADS=4

# Monitoring & Optimization
ENABLE_OPTIMIZATION=true
OPTIMIZATION_SCHEDULE="0 2 * * *"
ACCURACY_ALERT_THRESHOLD=0.65

# Demo Mode
DEMO_MODE=false
USE_CACHED_RESULTS=true
MOCK_API_CALLS=false

# Paths
DATA_DIR=./data
MODELS_DIR=./models
LOGS_DIR=./logs